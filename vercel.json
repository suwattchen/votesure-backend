{
  "functions": { "api/**/*.js": { "runtime": "nodejs20.x" } },
  "routes": [
    {
      "src": "/api/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "https://vote.infinityfreeapp.com",
        "Access-Control-Allow-Methods": "GET,POST,OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type,Authorization"
      },
      "continue": true
    }
  ]
}
